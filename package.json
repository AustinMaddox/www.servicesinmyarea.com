{
  "name": "servicesinmyarea",
  "version": "1.0.0",
  "description": "Services In My Area",
  "repository": "https://github.com/AustinMaddox/www.servicesinmyarea.com.git",
  "author": "Austin Maddox",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^9.4.7",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.3",
    "style-loader": "^0.23.1",
    "tailwindcss": "^0.7.4",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "scripts": {
    "dev:tailwind-cli:build": "npx tailwind build ./src/sass/tailwind-source.scss -c tailwind.config.js -o src/css/tailwind-output.css",
    "dev:webpack:build": "cross-env NODE_ENV=development webpack",
    "dev:webpack:server": "cross-env NODE_ENV=development webpack-dev-server --hot --inline",
    "dev:webpack:watch": "cross-env NODE_ENV=development webpack --watch",
    "prettier:check": "prettier --list-different \"src/**/*.{css,js,json,jsx}\"",
    "prettier:format": "prettier --write \"src/**/*.{css,js,json,jsx}\"",
    "prod:webpack:build": "cross-env NODE_ENV=production webpack --no-progress"
  }
}
